# This plots the data from 8th July 2002

set pointsize 0.01
set style data xyerrorbars
set key below
set size 1, 2

set terminal pslatex
set color
set xlabel "$I_b/\\ut{nA}$"
set ylabel "$f_d/\\ut{Hz}$"

set output '20020708_Polarimetry.tex'
plot '20020708_Polarimetry.dat' using 5:($2 < 1.5 ? $8 : 1/0):6:9 title 'Detector 1', '20020708_Polarimetry.dat' using 5:($2 >= 1.5 ? $8 : 1/0):6:9 title 'Detector 2'

set terminal png
set color
set xlabel "Ib/nA"
set ylabel "fd/Hz"

set output '20020708_Polarimetry.png'
plot '20020708_Polarimetry.dat' using 5:($2 < 1.5 ? $8 : 1/0):6:9 title 'Detector 1', '20020708_Polarimetry.dat' using 5:($2 >= 1.5 ? $8 : 1/0):6:9 title 'Detector 2'
@


17.2
log
@Syntax updated for newer Gnuplot versions.
@
text
@d26 2
a27 1
set terminal pslatex color
d34 2
a35 1
set terminal png color
@


