set ylabel "$R_d/\\ut{Hz}$"
d35 1
a35 1
set ylabel "Rd/Hz"
@


9.1
log
@Early summer inference script changed to reflect differences
between this experiment and the autumn one.
More summer data graphs added.
@
text
@a20 2
#load 'postsummer.gp'

d24 1
a24 1
set size 1, 1.15
a38 14

set terminal pslatex color
set xlabel "$I_b/\\ut{nA}$"
set ylabel "$R_a/\\ut{Hz}$"

set output '20020708_perfect.tex'
#plot '20020708_Polarimetry.dat' using 5:($2 < 1.5 ? $0 < 29.5 ? $8/(P_0_0_0*eta_0) : $8/(P_0_1_0*eta_0) : 1/0):6:($0 < 29.5 ? $9/(P_0_0_0*eta_0) : $9/(P_0_1_0*eta_0)) title 'Detector 1', '20020708_Polarimetry.dat' using 5:($2 >= 1.5 ? $0 < 29.5 ? $8/(P_0_0_1*eta_1) : $8/(P_0_1_1*eta_1) : 1/0):6:($0 < 29.5 ? $9/(P_0_0_1*eta_1) : $9/(P_0_1_1*eta_1)) title 'Detector 2'

set terminal png color
set xlabel "Ib/nA"
set ylabel "Ha/Hz"

set output '20020708_perfect.png'
#plot '20020708_Polarimetry.dat' using 5:($2 < 1.5 ? $0 < 29.5 ? $8/(P_0_0_0*eta_0) : $8/(P_0_1_0*eta_0) : 1/0):6:($0 < 29.5 ? $9/(P_0_0_0*eta_0) : $9/(P_0_1_0*eta_0)) title 'Detector 1', '20020708_Polarimetry.dat' using 5:($2 >= 1.5 ? $0 < 29.5 ? $8/(P_0_0_1*eta_1) : $8/(P_0_1_1*eta_1) : 1/0):6:($0 < 29.5 ? $9/(P_0_0_1*eta_1) : $9/(P_0_1_1*eta_1)) title 'Detector 2'
@


8.1
log
@Inference script for summer 2002, non-UHV data added.
References to inferred parameters that are not yet ready commented out.
@
text
@d29 2
a30 2
set xlabel "$E_b/\\ut{eV}$"
set ylabel "$I_d/\\ut{fA}$"
d33 1
a33 1
plot '20020708_Polarimetry.dat' using 3:($2 < 1.5 ? $8 : 1/0):4:9 title 'Detector 1', '20020708_Polarimetry.dat' using 3:($2 >= 1.5 ? $8 : 1/0):4:9 title 'Detector 2'
d36 2
a37 2
set xlabel "Eb/eV"
set ylabel "Id/fA"
d40 1
a40 1
plot '20020708_Polarimetry.dat' using 3:($2 < 1.5 ? $8 : 1/0):4:9 title 'Detector 1', '20020708_Polarimetry.dat' using 3:($2 >= 1.5 ? $8 : 1/0):4:9 title 'Detector 2'
d43 2
a44 2
set xlabel "$E_b/\\ut{eV}$"
set ylabel "$I_a/\\ut{fA}$"
d47 1
a47 1
#plot '20020708_Polarimetry.dat' using 3:($2 < 1.5 ? $0 < 29.5 ? $8/(P_0_0_0*eta_0) : $8/(P_0_1_0*eta_0) : 1/0):4:($0 < 29.5 ? $9/(P_0_0_0*eta_0) : $9/(P_0_1_0*eta_0)) title 'Detector 1', '20020708_Polarimetry.dat' using 3:($2 >= 1.5 ? $0 < 29.5 ? $8/(P_0_0_1*eta_1) : $8/(P_0_1_1*eta_1) : 1/0):4:($0 < 29.5 ? $9/(P_0_0_1*eta_1) : $9/(P_0_1_1*eta_1)) title 'Detector 2'
d50 2
a51 2
set xlabel "Eb/eV"
set ylabel "Ia/fA"
d54 1
a54 1
#plot '20020708_Polarimetry.dat' using 3:($2 < 1.5 ? $0 < 29.5 ? $8/(P_0_0_0*eta_0) : $8/(P_0_1_0*eta_0) : 1/0):4:($0 < 29.5 ? $9/(P_0_0_0*eta_0) : $9/(P_0_1_0*eta_0)) title 'Detector 1', '20020708_Polarimetry.dat' using 3:($2 >= 1.5 ? $0 < 29.5 ? $8/(P_0_0_1*eta_1) : $8/(P_0_1_1*eta_1) : 1/0):4:($0 < 29.5 ? $9/(P_0_0_1*eta_1) : $9/(P_0_1_1*eta_1)) title 'Detector 2'
@


7.1
log
@Summer 2002 data added.
Facilities for graphing some summer 2002 data added.
@
text
@d21 1
a21 1
load 'postsummer.gp'
d33 1
a33 1
plot '20020708_Polarimetry.dat' using 3:($1 >= 0 && $2 < 1.5 ? $8 : 1/0):4:9 title 'Magnetization up, detector 1', '20020708_Polarimetry.dat' using 3:($1 >= 0 && $2 >= 1.5 ? $8 : 1/0):4:9 title 'Magnetization up, detector 2', '20020708_Polarimetry.dat' using 3:($1 < 0 && $2 < 1.5 ? $8 : 1/0):4:9 title 'Magnetization down, detector 1', '20020708_Polarimetry.dat' using 3:($1 < 0 && $2 >= 1.5 ? $8 : 1/0):4:9 title 'Magnetization down, detector 2'
d47 1
a47 1
plot '20020708_Polarimetry.dat' using 3:($2 < 1.5 ? $0 < 29.5 ? $8/(P_0_0_0*eta_0) : $8/(P_0_1_0*eta_0) : 1/0):4:($0 < 29.5 ? $9/(P_0_0_0*eta_0) : $9/(P_0_1_0*eta_0)) title 'Detector 1', '20020708_Polarimetry.dat' using 3:($2 >= 1.5 ? $0 < 29.5 ? $8/(P_0_0_1*eta_1) : $8/(P_0_1_1*eta_1) : 1/0):4:($0 < 29.5 ? $9/(P_0_0_1*eta_1) : $9/(P_0_1_1*eta_1)) title 'Detector 2'
d54 1
a54 1
plot '20020708_Polarimetry.dat' using 3:($2 < 1.5 ? $0 < 29.5 ? $8/(P_0_0_0*eta_0) : $8/(P_0_1_0*eta_0) : 1/0):4:($0 < 29.5 ? $9/(P_0_0_0*eta_0) : $9/(P_0_1_0*eta_0)) title 'Detector 1', '20020708_Polarimetry.dat' using 3:($2 >= 1.5 ? $0 < 29.5 ? $8/(P_0_0_1*eta_1) : $8/(P_0_1_1*eta_1) : 1/0):4:($0 < 29.5 ? $9/(P_0_0_1*eta_1) : $9/(P_0_1_1*eta_1)) title 'Detector 2'
@
