11.1
log
@Graphs of results from last-minute experiments added.
Typographical errors corrected.
@
text
@a20 2
#load 'postsummer.gp'

d28 1
a28 1
set ylabel "$f_d/\\ut{fA}$"
a50 14

set terminal pslatex color
set xlabel "$E_b/\\ut{eV}$"
set ylabel "$I_a/\\ut{fA}$"

set output '20020923_perfect.tex'
#plot '20020923_Polarimetry_2.dat' using 3:($1 >= 0 && $2 < 1.5 && $5 < 3000 ? $8/(P_2_0_0*eta_0) :  1/0):4:($9/(P_2_0_0*eta_0)) title "Magnetization up, detector 1, $I_b = (2\\pm{}0.029)\\ut{\\mu{}A}$", '20020923_Polarimetry_2.dat' using 3:($1 >= 0 && $2 < 1.5 && $5 >= 3000 && $5 < 5000 ? $8/(P_2_0_0*eta_0) : 1/0):4:($9/(P_2_0_0*eta_0)) title "Magnetization up, detector 1, $I_b = (4\\pm{}0.029)\\ut{\\mu{}A}$", '20020923_Polarimetry_2.dat' using 3:($1 >= 0 && $2 < 1.5 && $5 >= 5000 ? $8/(P_2_0_0*eta_0) : 1/0):4:($9/(P_2_0_0*eta_0)) title "Magnetization up, detector 1, $I_b = (6\\pm{}0.029)\\ut{\\mu{}A}$", '20020923_Polarimetry_2.dat' using 3:($1 >= 0 && $2 >= 1.5 && $5 < 3000 ? $8/(P_2_0_1*eta_1) : 1/0):4:($9/(P_2_0_1*eta_1)) title "Magnetization up, detector 2, $I_b = (2\\pm{}0.029)\\ut{\\mu{}A}$", '20020923_Polarimetry_2.dat' using 3:($1 >= 0 && $2 >= 1.5 && $5 >= 3000 && $5 < 5000 ? $8/(P_2_0_1*eta_1) : 1/0):4:($9/(P_2_0_1*eta_1)) title "Magnetization up, detector 2, $I_b = (4\\pm{}0.029)\\ut{\\mu{}A}$", '20020923_Polarimetry_2.dat' using 3:($1 >= 0 && $2 >= 1.5 && $5 >= 5000 ? $8/(P_2_0_1*eta_1) : 1/0):4:($9/(P_2_0_1*eta_1)) title "Magnetization up, detector 2, $I_b = (6\\pm{}0.029)\\ut{\\mu{}A}$", '20020923_Polarimetry_2.dat' using 3:($1 < 0 && $2 < 1.5 && $5 < 3000 ? $8/(P_2_1_0*eta_0) : 1/0):4:($9/(P_2_1_0*eta_0)) title "Magnetization down, detector 1, $I_b = (2\\pm{}0.029)\\ut{\\mu{}A}$", '20020923_Polarimetry_2.dat' using 3:($1 < 0 && $2 < 1.5 && $5 >= 3000 && $5 < 5000 ? $8/(P_2_1_0*eta_0) : 1/0):4:($9/(P_2_1_0*eta_0)) title "Magnetization down, detector 1, $I_b = (4\\pm{}0.029)\\ut{\\mu{}A}$", '20020923_Polarimetry_2.dat' using 3:($1 < 0 && $2 < 1.5 && $5 >= 5000 ? $8/(P_2_1_0*eta_0) : 1/0):4:($9/(P_2_1_0*eta_0)) title "Magnetization down, detector 1, $I_b = (6\\pm{}0.029)\\ut{\\mu{}A}$", '20020923_Polarimetry_2.dat' using 3:($1 < 0 && $2 >= 1.5 && $5 < 3000 ? $8/(P_2_1_1*eta_1) : 1/0):4:($9/(P_2_1_1*eta_1)) title "Magnetization down, detector 2, $I_b = (2\\pm{}0.029)\\ut{\\mu{}A}$", '20020923_Polarimetry_2.dat' using 3:($1 < 0 && $2 >= 1.5 && $5 >= 3000 && $5 < 5000 ? $8/(P_2_1_1*eta_1) : 1/0):4:($9/(P_2_1_1*eta_1)) title "Magnetization down, detector 2, $I_b = (4\\pm{}0.029)\\ut{\\mu{}A}$", '20020923_Polarimetry_2.dat' using 3:($1 < 0 && $2 >= 1.5 && $5 >= 5000 ? $8/(P_2_1_1*eta_1) : 1/0):4:($9/(P_2_1_1*eta_1)) title "Magnetization down, detector 2, $I_b = (6\\pm{}0.029)\\ut{\\mu{}A}$"

set terminal png color
set xlabel "Eb/eV"
set ylabel "Ia/fA"

set output '20020923_perfect.png'
#plot '20020923_Polarimetry_2.dat' using 3:($1 >= 0 && $2 < 1.5 && $5 < 3000 ? $8/(P_2_0_0*eta_0) :  1/0):4:($9/(P_2_0_0*eta_0)) title "Magnetization up, detector 1, Ib = (2+/-0.029)muA", '20020923_Polarimetry_2.dat' using 3:($1 >= 0 && $2 < 1.5 && $5 >= 3000 && $5 < 5000 ? $8/(P_2_0_0*eta_0) : 1/0):4:($9/(P_2_0_0*eta_0)) title "Magnetization up, detector 1, Ib = (4+/-0.029)muA", '20020923_Polarimetry_2.dat' using 3:($1 >= 0 && $2 < 1.5 && $5 >= 5000 ? $8/(P_2_0_0*eta_0) : 1/0):4:($9/(P_2_0_0*eta_0)) title "Magnetization up, detector 1, Ib = (6+/-0.029)muA", '20020923_Polarimetry_2.dat' using 3:($1 >= 0 && $2 >= 1.5 && $5 < 3000 ? $8/(P_2_0_1*eta_1) : 1/0):4:($9/(P_2_0_1*eta_1)) title "Magnetization up, detector 2, Ib = (2+/-0.029)muA", '20020923_Polarimetry_2.dat' using 3:($1 >= 0 && $2 >= 1.5 && $5 >= 3000 && $5 < 5000 ? $8/(P_2_0_1*eta_1) : 1/0):4:($9/(P_2_0_1*eta_1)) title "Magnetization up, detector 2, Ib = (4+/-0.029)muA", '20020923_Polarimetry_2.dat' using 3:($1 >= 0 && $2 >= 1.5 && $5 >= 5000 ? $8/(P_2_0_1*eta_1) : 1/0):4:($9/(P_2_0_1*eta_1)) title "Magnetization up, detector 2, Ib = (6+/-0.029)muA", '20020923_Polarimetry_2.dat' using 3:($1 < 0 && $2 < 1.5 && $5 < 3000 ? $8/(P_2_1_0*eta_0) : 1/0):4:($9/(P_2_1_0*eta_0)) title "Magnetization down, detector 1, Ib = (2+/-0.029)muA", '20020923_Polarimetry_2.dat' using 3:($1 < 0 && $2 < 1.5 && $5 >= 3000 && $5 < 5000 ? $8/(P_2_1_0*eta_0) : 1/0):4:($9/(P_2_1_0*eta_0)) title "Magnetization down, detector 1, Ib = (4+/-0.029)muA", '20020923_Polarimetry_2.dat' using 3:($1 < 0 && $2 < 1.5 && $5 >= 5000 ? $8/(P_2_1_0*eta_0) : 1/0):4:($9/(P_2_1_0*eta_0)) title "Magnetization down, detector 1, Ib = (6+/-0.029)muA", '20020923_Polarimetry_2.dat' using 3:($1 < 0 && $2 >= 1.5 && $5 < 3000 ? $8/(P_2_1_1*eta_1) : 1/0):4:($9/(P_2_1_1*eta_1)) title "Magnetization down, detector 2, Ib = (2+/-0.029)muA", '20020923_Polarimetry_2.dat' using 3:($1 < 0 && $2 >= 1.5 && $5 >= 3000 && $5 < 5000 ? $8/(P_2_1_1*eta_1) : 1/0):4:($9/(P_2_1_1*eta_1)) title "Magnetization down, detector 2, Ib = (4+/-0.029)muA", '20020923_Polarimetry_2.dat' using 3:($1 < 0 && $2 >= 1.5 && $5 >= 5000 ? $8/(P_2_1_1*eta_1) : 1/0):4:($9/(P_2_1_1*eta_1)) title "Magnetization down, detector 2, Ib = (6+/-0.029)muA"
