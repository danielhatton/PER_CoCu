# 330, Boston, MA 02111-1307 USA

# This plots the 2nd data set from 5th December 2001

set pointsize 0.01
set style data xyerrorbars
set size 1, 2
set key below

set terminal pslatex
set color
set xlabel "$E_b/\\ut{eV}$"
set ylabel "$I_d/\\ut{fA}$"

set output '20011205_Polarimetry_2.tex'
plot '20011205_Polarimetry_2.dat' using 3:($1 >= 0 && $2 < 1.5 ? $8 : 1/0):4:9 title 'Magnetization up, detector 1', '20011205_Polarimetry_2.dat' using 3:($1 >= 0 && $2 >= 1.5 ? $8 : 1/0):4:9 title 'Magnetization up, detector 2', '20011205_Polarimetry_2.dat' using 3:($1 < 0 && $2 < 1.5 ? $8 : 1/0):4:9 title 'Magnetization down, detector 1', '20011205_Polarimetry_2.dat' using 3:($1 < 0 && $2 >= 1.5 ? $8 : 1/0):4:9 title 'Magnetization down, detector 2'

set terminal png
set color
set xlabel "Eb/eV"
set ylabel "Id/fA"

set output '20011205_Polarimetry_2.png'
plot '20011205_Polarimetry_2.dat' using 3:($1 >= 0 && $2 < 1.5 ? $8 : 1/0):4:9 title 'Magnetization up, detector 1', '20011205_Polarimetry_2.dat' using 3:($1 >= 0 && $2 >= 1.5 ? $8 : 1/0):4:9 title 'Magnetization up, detector 2', '20011205_Polarimetry_2.dat' using 3:($1 < 0 && $2 < 1.5 ? $8 : 1/0):4:9 title 'Magnetization down, detector 1', '20011205_Polarimetry_2.dat' using 3:($1 < 0 && $2 >= 1.5 ? $8 : 1/0):4:9 title 'Magnetization down, detector 2'
@


