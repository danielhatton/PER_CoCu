# You should have received a copy of the GNU General Public License
# along with this program (in the file copying.programs;) if not,
# write to the Free Software Foundation, Inc., 59 Temple Place, Suite
# 330, Boston, MA 02111-1307 USA

# This plots the convergence of the detection efficiency of channeltron 1.

set pointsize 0.01
set style data lines
set key below
set size 1, 2

set terminal pslatex
set color
set xlabel "$i$"
set ylabel "$\\bar{\\eta_1}^{(\\textrm{pos}, n, i)}$ or $\\sqrt{\\bar{\\eta_1^2}^{(\\textrm{pos}, n, i)}}$"

set output 'eta1-converge.tex'
plot 'null_params' using ($0+1):30 title "$\\bar{\\eta_1}^{(\\textrm{pos}, N, i)}$", 'null_sparams' using ($0+1):(sqrt($30)) title "$\\sqrt{\\bar{\\eta_1^2}^{(\\textrm{pos}, N, i)}}$", 'main_params' using ($0+1):30 title "$\\bar{\\eta_1}^{(\\textrm{pos}, M, i)}$", 'main_sparams' using ($0+1):(sqrt($30)) title "$\\sqrt{\\bar{\\eta_1^2}^{(\\textrm{pos}, M, i)}}$"

set terminal png
set color
set xlabel "i"
set ylabel "eta1bar(pos, n, i) or sqrt(eta1^2bar(pos, n, i))"

set output 'eta1-converge.png'
plot 'null_params' using ($0+1):30 title "eta1bar(pos, N, i)", 'null_sparams' using ($0+1):(sqrt($30)) title "sqrt(eta1^2bar(pos, N, i))", 'main_params' using ($0+1):30 title "eta1bar(pos, M, i)", 'main_sparams' using ($0+1):(sqrt($30)) title "sqrt(eta1^2bar(pos, M, i))"
@


15.4
log
